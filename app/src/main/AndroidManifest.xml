<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="votacion.presidencia.petro.testigoscolombiahumana">

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-feature
        android:name="android.hardware.camera"
        android:required="true" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="petro.presidencia.votacion.vuelta2.actividades.guiaActivity"
            android:parentActivityName="petro.presidencia.votacion.vuelta2.principalActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="petro.presidencia.votacion.vuelta2.actividades.testigos.anomaliasActivity"
            android:parentActivityName="petro.presidencia.votacion.vuelta2.principalActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="petro.presidencia.votacion.vuelta2.actividades.testigos.votacionActivity"
            android:parentActivityName="petro.presidencia.votacion.vuelta2.principalActivity"
            android:screenOrientation="portrait"

            />

        <activity
            android:name="petro.presidencia.votacion.vuelta2.actividades.forgorActivity"
            android:parentActivityName="petro.presidencia.votacion.vuelta2.principalActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="petro.presidencia.votacion.vuelta2.actividades.notiActivity"
            android:parentActivityName="petro.presidencia.votacion.vuelta2.principalActivity" />

        <activity android:name="petro.presidencia.votacion.vuelta2.principalActivity" android:screenOrientation="portrait">
        </activity>
        <activity android:name="petro.presidencia.votacion.splash" android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>